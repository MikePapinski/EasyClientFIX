<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>MikePapinski</Authors>
    <Company>FXbox</Company>
    <PackageId>EasyClientFIX</PackageId>
    <Description>Simple implementation of FIX protocol client session without any external libraries. All based on TCP. Feel free to contribute</Description>
    <Copyright />
    <RepositoryUrl>https://github.com/MikePapinski/EasyClientFIX</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageReleaseNotes>Moved classes to nuget package</PackageReleaseNotes>
    <PackageIcon>my_face_medium.png</PackageIcon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product>EasyClientFIX</Product>
    <Version>1.0.1003</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\my_face_medium.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
